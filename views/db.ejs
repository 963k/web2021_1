<!Doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Show message</title>
  </head>
  <body>
    <h2><%=data[0].name%>の詳細</h2>
    <table>
      <% for (let row of data){%>
        <tr><td>ID</td><td><%= row.id %></td></tr>
        <tr><td>income</td><td><%= row.income %></td></tr>
        <tr><td>maker</td><td><%= row.maker %></td></tr>
      <%}%>      
        
    </table>
  </body>
</html>